{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "MONGODB_USERNAME": "@mongodb_username",
      "MONGODB_PASSWORD": "@mongodb_password"
    }
  }
  